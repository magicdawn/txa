{
  "name": "txa",
  "version": "0.0.1",
  "description": "osascript -l TypeScript",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "prepare": "husky install",
    "test-cover": "nyc --reporter=lcov --reporter=text mocha",
    "report-cover": "codecov",
    "build": "rm -rf lib; rm tsconfig.tsbuildinfo; tsc",
    "prepublishOnly": "npm run build",
    "start": "ts-node ./src/index.ts",
    "start:debug": "node --inspect-brk -r ts-node/register ./src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/magicdawn/txa.git"
  },
  "keywords": [
    "osascript",
    "osacompile",
    "automation",
    "jxa",
    "txa"
  ],
  "author": "magicdawn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/magicdawn/txa/issues"
  },
  "homepage": "https://github.com/magicdawn/txa#readme",
  "devDependencies": {
    "@magicdawn/prettier-config": "^0.0.2",
    "@swc/core": "^1.3.14",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mocha": "^6.2.3",
    "nyc": "^14.1.1",
    "prettier": "^2.7.1",
    "should": "^13.2.3",
    "typescript": "^4.8.4",
    "@swc/helpers": "^0.4.12",
    "ts-node": "^10.9.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,less,md}": [
      "prettier --write"
    ]
  }
}
